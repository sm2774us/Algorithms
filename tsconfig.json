{
    "compilerOptions": {
      "lib": ["esnext"],
      "strict": true,
      "noUnusedLocals": true,
      "noUnusedParameters": true,
      "noImplicitReturns": true,
      "noFallthroughCasesInSwitch": true,
      "downlevelIteration": true,
      "plugins": [{ "name": "typescript-deno-plugin" }],
      "target": "es5"
    },
    "include": ["src/main/typescript/**/*"],
    "exclude": ["node_modules", "src/main/typescript/**/*.spec.ts"]
  }